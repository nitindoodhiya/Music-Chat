    style.
        body div img{
        width: 60%;
        height: 60%;
        }
        body div li{
        list-style-type: none;
        }
  body
    nav.navbar.navbar-default.custom-header
      .container-fluid
        .navbar-header
          a.navbar-brand.navbar-link(href='#')
            li  Companylogo 
                            
          button.navbar-toggle.collapsed(data-toggle='collapse', data-target='#navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
                      
        #navbar-collapse.collapse.navbar-collapse
          ul.nav.navbar-nav.links
            li(role='presentation')
              a(href='/#{userid}/listsongs') YourSongs 
                                  
            li(role='presentation')
              a(href='/#{userid}/showualbums') Albums 
                                  
            li(role='presentation')
              a(href='#')  AllSongs
                                  
            li(role='presentation')
              a(href='/#{userid}/showallalbums') AllAlbums 
                                  
            li(role='presentation')
              a(href='/#{userid}/showusers')  AllUsers
            li(role='presentation')
              a.custom-navbar(href='#') Notification
                 span.badge new
                            
          ul.nav.navbar-nav.navbar-right
            li.dropdown
              a.dropdown-toggle(data-toggle='dropdown', aria-expanded='false', href='#')
                span.caret
                img.dropdown-image(src='/assets/img/avatar.jpg')
                                        
              ul.dropdown-menu.dropdown-menu-right(role='menu')
                li.active(role='presentation')
                  a(href='/logout') Logout 
       

    .container
      form
        input#myText(type='text')
        button.btn.btn-default.mybtn(onclick='myfunction({item})') search
        div(style='background-color:rgb(35, 37, 37)')
          ul.listingul
            -for(var i=0;i<item.length;i++){
                li=item[i].SongName 
            -}
    footer
      .row
        .col-md-4.col-sm-6.footer-navigation
          h3
            a(href='#')
                Company
              span logo 
                            
          p.links
            a(href='#') Home
            strong  &middot; 
            a(href='#') Blog
            strong  &middot; 
            a(href='#') Pricing
            strong  &middot; 
            a(href='#') About
            strong  &middot; 
            a(href='#') Faq
            strong  &middot; 
            a(href='#') Contact
                            
          p.company-name Company Name &copy; 2015 
                      
        .col-md-4.col-sm-6.footer-contacts
          div
            span.fa.fa-map-marker.footer-contacts-icon  
                                  
            p
              span.new-line-span 21 Revolution Street
                 Paris, France
                            
          div
            i.fa.fa-phone.footer-contacts-icon
                                  
            p.footer-center-info.email.text-left  +1 555 123456
                            
          div
            i.fa.fa-envelope.footer-contacts-icon
                                  
            p
              a(href='#', target='_blank') support@company.com
                      
        .clearfix.visible-sm-block
                      
        .col-md-4.footer-about
          h4 About the company
                            
          p
              Lorem ipsum dolor sit amet, consectateur adispicing elit. Fusce euismod convallis velit, eu auctor lacus vehicula sit amet.
                            
          .social-links.social-icons
            a(href='#')
              i.fa.fa-facebook
            a(href='#')
              i.fa.fa-twitter
            a(href='#')
              i.fa.fa-linkedin
            a(href='#')
              i.fa.fa-github
          
